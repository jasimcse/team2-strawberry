<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0"
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
      http://java.sun.com/xml/ns/javaee
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <converter>
		<converter-id>KeyToStringConvertor</converter-id>
		<converter-class>controller.common.ConvertorKeyString</converter-class>
	</converter>
	<converter>
		<converter-for-class>com.google.appengine.api.datastore.Key</converter-for-class>
		<converter-class>controller.common.ConvertorKeyString</converter-class>
	</converter>
	<lifecycle>
		<phase-listener>controller.common.ForceSessionSerializationPhaseListener</phase-listener>
	</lifecycle>
      
</faces-config>